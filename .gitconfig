[color]
	ui = auto

[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green

[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold

[color "status"]
	added = yellow
	changed = green
	untracked = cyan

[alias]
	st = status
	lo = log --decorate --stat
	ci = commit
	br = branch
	co = checkout
	df = diff
	lg = log -p

[user]
	email =
	name =
